datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Referral {
  id              Int      @id @default(autoincrement())
  yourName        String
  yourEmail       String
  yourPhone       String
  refereeName     String
  refereeEmail    String
  refereePhone    String
  courseInterested String
  createdAt       DateTime @default(now())
}
